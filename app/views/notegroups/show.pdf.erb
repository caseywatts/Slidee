  <body onload='number_pages'>
    <div class="firstpage" id="firstpage">
      <h1 class="slidee">Slidee Class Notes</h1>
      <h1 class="title"><%= @notegroup.presentation.deckoriginal_file_name %></h1>
      <h3 class="dl_time">Downloaded <% time = Time.new %><%= time.strftime("%B %d, %Y") %></h1>
      </br>
      </br>
    <% if !@notegroup.everyonebutme.empty?%>
      <h2 class="username" style="font-weight:normal;"><span style="font-weight:bold"><%= User.current.fullname %></span> with</h2>
      <p class="collaborators">
      <ul>
      <% @notegroup.everyonebutme.each do |user|%>
        <li><%= user.fullname %></li>
      <% end %>
      </ul>
      </p>
    <% else %>
      <h2 class="username"><%= User.current.fullname %></h2>
    <%end%>

    </div>
    <div id="container">
	    <% @slide_number = 1 %>
      <%= render :partial => "notegroups/slidewithnote", :collection => @notegroup.presentation.slides, :locals => {:notegroup => @notegroup} %>
    </div>
  </body>
