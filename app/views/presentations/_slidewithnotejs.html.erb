<script type="text/javascript">
var length = <%=slideset.length%>;
var slideset = <%=slideset.to_json%>;

//I preload only the first five slides, wait a second, then do the rest
if (length>=5)
  {
  var i=0;
  for (i=0;i<5;i++)
    {
    $("#note"+ slideset[i]).pad({'padId':("LectureTogetherNote" + slideset[i]),'width':'100%','height':'100%','showControls':true,'userName':'caseywatts'});
    }
  setTimeout(function(){
      for (i=5;i<=length;i++)
        {
        $("#note"+ slideset[i]).pad({'padId':("LectureTogetherNote" + slideset[i]),'width':'100%','height':'100%','showControls':true,'userName':'caseywatts'});
        }
    }
    ,5000
  );
  }
else
  {
  var i=0;
  for (i=0;i<=5;i++)
    {
    $("#note"+ slideset[i]).pad({'padId':("LectureTogetherNote" + slideset[i]),'width':'100%','height':'100%','showControls':true,'userName':'caseywatts'});
    }

  }

</script>
